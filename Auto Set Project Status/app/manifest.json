{
    "manifest_version": 2,

    "name": "Auto Set Project Status",
    "description": "This extension auto set project status for Dotsquares EMS.",
    "version": "1.1",
    "icons": {
        "48": "img/icon48.png",
        "96": "img/icon96.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://dsems.projectstatus.in/*",
        "storage"
    ],
    "browser_action": {
        "default_icon": {
            "48": "img/icon48.png",
            "96": "img/icon96.png",
            "16": "img/icon.png"
        },
        "default_title": "Set Project Status",
        "default_popup": "html/setprojectstatuspopup.html"
    },
    "options_page": "html/options.html",
    "minimum_chrome_version": "6.0.0.0"
}